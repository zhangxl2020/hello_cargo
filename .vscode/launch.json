    "configurations": [
        {
            "name": "Debug executable 'hello_cargo'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "run",
                    "--bin=hello_cargo"
                ]
            },
            // 程序参数：在这里填写运行时参数，例如 ["./", "errors.log"]
            "args": [],
            // 启用 Rust 的回溯以便在 panic 时获得堆栈信息
            "env": {
                "RUST_BACKTRACE": "1"
            }
        },
        {
            "name": "Debug unit tests in executable 'hello_cargo'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--bin=hello_cargo"
                ]
            }
        }
    ]
}